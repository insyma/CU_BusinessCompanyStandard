{
  "compileOnSave": true,
  "typeAcquisition": {
    "enable": true
  },
  "compilerOptions": {
    "allowJs": true,
    "noEmit": false,
    "outDir": "wwwroot/js",
    "declaration": false,
    "lib": [ "dom", "es6", "dom.iterable" ],
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "noImplicitUseStrict": true,
    "noEmitOnError": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "target": "es6"
  },
  "files": [
    "wwwroot/ts/typescript.ts"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}